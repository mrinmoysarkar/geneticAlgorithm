clear all;
close all;

l=5;
m_min = 1;
m_max = 100;%^100;
m1_prev = 0;
m2_prev = 0;
while 1
    m1 = ceil((m_max-m_min)/3) + 1;
    m2 = ceil((2*(m_max-m_min))/3) + 1;
    f1 = f(m1,l);
    f2 = f(m2,l);
    if m1 - m2
        fprintf('Optimal Population Size m* = %d \n', m2);
        break;
    elseif f1 > f2
        m_max = m2;
    else
        m_min = m1;
    end
    m1_prev = m1;
    m2_prev = m2;
    fprintf('m1=%d  m2=%d\n',m1,m2);
    pause(2)
end
